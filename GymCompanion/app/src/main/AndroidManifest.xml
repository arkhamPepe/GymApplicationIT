<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="se.chalmers.group22.gymcompanion">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".View.Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".View.Schedule.ScheduleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".StartRoutineActivity"
                android:exported="true">
        </activity>
        <activity android:name=".View.Browse.BrowseActivity">
        </activity>
        <activity android:name=".View.Statistics.StatisticsActivity">
        </activity>
        <activity android:name=".View.MyRoutines.MyRoutinesActivity">
        </activity>
        <activity android:name=".View.Progress.ProgressActivity">
        </activity>
    </application>

</manifest>